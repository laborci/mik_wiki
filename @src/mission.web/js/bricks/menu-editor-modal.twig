<div class="modalify">

	<header>
		<i class="inverse fal fa-indent"></i>
		<label>Menu Editor</label>
		<nav>
			<button (save)><i class="fal fa-save"></i></button>
			<button (close)><i class="fal fa-times"></i></button>
		</nav>
	</header>

	<section>
		<div (editor) is="menu-editor">{{ menu|json_encode }}</div>
	</section>



</div>